 <% flash.each do |key, value| %>
    <div class="alert alert-<%= key %>">
      <a href="#" data-dismiss="alert" class="close">Ã—</a>
      </br>
      <% if value.is_a? String %>
        <li>
          <%= value %>
        </li>
      <% else %>
        <% value.each do |value_item| %>
          <li>
            <%= value_item %>
          </li>
          <br>
        <% end %>
      <% end %>

      </ul>
    </div>
  <% end %>